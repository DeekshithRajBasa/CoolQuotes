<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#D7CCC8</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">false</item>
        <item name="android:textColorPrimary">@color/colorPrimaryText</item>
        <item name="android:listDivider">@color/brownFaded</item>

    </style>

    <style name="BlueTheme" parent="Theme.AppCompat.Light.DarkActionBar">

        <item name="colorPrimary">@color/bluecolorPrimaryLight</item>
        <item name="colorPrimaryDark">#0288D1</item>
        <item name="colorAccent">#607D8B</item>


    </style>


</resources>
